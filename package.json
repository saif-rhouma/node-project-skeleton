{
  "name": "node-project-skeleton",
  "version": "0.0.1",
  "description": "Node Express MVC Project Skeleton By Saifeddine RHOUMA",
  "scripts": {
    "build": "babel ./src --out-dir ./dist",
    "start": "node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --config ./nodemon.json --exec babel-node src/server.js",
    "debug": "npm run dev -- --inspect",
    "test": "jest --runInBand",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saif-rhouma/node-project-skeleton.git"
  },
  "keywords": [
    "Skeleton",
    "Node",
    "Express",
    "MVC"
  ],
  "author": "Saifeddine RHOUMA",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saif-rhouma/node-project-skeleton/issues"
  },
  "homepage": "https://github.com/saif-rhouma/node-project-skeleton#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "morgan": "^1.10.0",
    "swagger-cli": "^4.0.4",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.4",
    "babel-jest": "^29.1.2",
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "supertest": "^6.3.0"
  }
}
